{
    "presets": [
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties"
        ]
    ],
    "env": {
        "es6": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "node": 8
                        }
                    }
                ]
            ],
            "plugins": [
                "transform-class-properties"
            ]
        },
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "node": 8
                        }
                    }
                ]
            ],
            "plugins": [
                "transform-class-properties"
            ]
        }
    }
}
